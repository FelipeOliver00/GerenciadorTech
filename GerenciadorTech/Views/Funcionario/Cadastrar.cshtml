@model Funcionario

@{
    ViewData["Title"] = "Cadastrar";
}

<h1>Cadastrar</h1>

@if (TempData["msg"] != null)
{
<div class="alert alert-success">
    @TempData["msg"]
</div>
}
<form asp-action="Cadastrar" asp-controller="Funcionario">

    <!-- Adiciona o conteúdo da página _Form.cshtml-->
    <partial name="_Form"/>

    <input type="submit" value="Cadastrar" class="btn btn-outline-primary" />
    <a asp-action="index" class="btn btn-outline-dark">Funcionários</a>

</form>

